{
  "name": "docker-prac-be",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./bin/www",
    "start-dev": "npm run swagger-binding & NODE_ENV=development nodemon ./bin/www",
